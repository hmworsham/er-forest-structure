}

@article{urban_forest_2000,
  title = {Forest Gradient Response in {{Sierran}} Landscapes: {{The}} Physical Template},
  author = {Urban, Dean L. and Miller, Carol and Halpin, Patrick N. and Stephenson, Nathan L.},
