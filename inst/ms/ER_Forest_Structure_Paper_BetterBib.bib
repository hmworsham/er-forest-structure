@article{underwood_using_2010,
  title = {Using Topography to Meet Wildlife and Fuels Treatment Objectives in Fire-Suppressed Landscapes},
  author = {Underwood, Emma C. and Viers, Joshua H. and Quinn, James F. and North, Malcolm},
