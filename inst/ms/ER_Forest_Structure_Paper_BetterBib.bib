  author = {Millikan, Pedro Henrique Karantino and Silva, Carlos Alberto and Rodriguez, Luiz Carlos Estraviz and {de Oliveira}, Tupiara Mergen and E Carvalho, Mariana Peres de Lima Chaves and E Carvalho, Samuel de P{\'a}dua Chaves},
