}

@article{bennett_climate_2020,
  title = {Climate More Important than Soils for Predicting Forest Biomass at the Continental Scale},
  author = {Bennett, Alison C. and Penman, Trent D. and Arndt, Stefan K. and Roxburgh, Stephen H. and Bennett, Lauren T.},
  year = {2020},
