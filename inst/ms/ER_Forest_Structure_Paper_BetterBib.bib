@article{milly_colorado_2020,
  title = {Colorado {{River}} Flow Dwindles as Warming-Driven Loss of Reflective Snow Energizes Evaporation},
  author = {Milly, P. C. D. and Dunne, K. A.},
  year = {2020},
  month = mar,
  journal = {Science},
  volume = {367},
