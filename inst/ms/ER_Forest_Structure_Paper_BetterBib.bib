  title = {Water Balance and Topography Predict Fire and Forest Structure Patterns},
  author = {Kane, Van R. and Lutz, James A. and Alina Cansler, C. and Povak, Nicholas A. and Churchill, Derek J. and Smith, Douglas F. and Kane, Jonathan T. and North, Malcolm P.},
