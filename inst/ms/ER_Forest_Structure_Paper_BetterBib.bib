  title = {Effect of Covariance between Ablation and Snow Water Equivalent on Depletion of Snow-Covered Area in a Forest},
  author = {Faria, D. A. and Pomeroy, J. W. and Essery, R. L. H.},
  year = {2000},
  month = oct,
  journal = {Hydrol. Process.},
  volume = {14},
  number = {15},
  pages = {2683--2695},
  issn = {0885-6087, 1099-1085},
  doi = {10.1002/1099-1085(20001030)14:15<2683::AID-HYP86>3.0.CO;2-N},
