@article{englhart_quantifying_2013,
  title = {Quantifying Dynamics in Tropical Peat Swamp Forest Biomass with Multi-Temporal {{LiDAR}} Datasets},
  author = {Englhart, Sandra and Jubanski, Juilson and Siegert, Florian},
  year = {2013},
  journal = {Remote Sens.},
  volume = {5},
